service cloud.firestore {
  match /databases/{database}/documents {
    function isSignedIn() {
      return request.auth.uid != null;
    }

    match /members/{id} {
      allow read;
    }

    match /operations/{id} {
      allow read;
    }

    match /uidToVideoMultiHashMap/{id} {
      allow read;
    }
  }
}
